{
    "name": "yaml-snippets",
    "displayName": "YAML Snippets for VS Code",
    "description": "The complete YAML snippet library for Cloudformation based on the AWS CloudFormation Resource Specification",
    "version": "1.1.0",
    "publisher": "dsteenman",
    "icon": "images/AWS-CloudFormation-Icon.png",
    "homepage": "https://dsteenman.github.io",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/dsteenman/cloudformation-yaml-snippets.git"
    },
    "engines": {
        "vscode": "^1.31.0"
    },
    "galleryBanner": {
        "color": "#2B2C33",
        "theme": "dark"
    },
    "categories": [
        "Snippets",
        "Other"
    ],
    "keywords": [
        "amazon",
        "aws",
        "cfn",
        "cloudformation",
        "multi-root ready",
        "property",
        "resource",
        "specification",
        "stack",
        "template",
        "yaml",
        "yml"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "yaml",
                "path": "./snippets/resource-types.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/extension.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/parameter-type.json"
            },
            {
                "language": "yaml",
                "path": "./snippets/custom.json"
            }
        ]
    }
}
